  "semanticTokenColors": {
    "comment": {
      "foreground": "#6b8370"
    },
    "string": {
      "foreground": "#f5e67a"
    },
    "number": {
      "foreground": "#d99ce8"
    },
    "keyword": {
      "foreground": "#a8e6a3"
    },
    "function": {
      "foreground": "#7dd3c0"
    },
    "parameter": {
      "foreground": "#f5c97a"
    },
    "property": {
      "foreground": "#a8e6a3"
    },
    "variable": {
      "foreground": "#d9ebe0"
    },
    "variable.readonly": {
      "foreground": "#7dc3e8"
    },
    "type": {
      "foreground": "#7dc3e8"
    },
    "enumMember": {
      "foreground": "#f5c97a"
    },
    "interface": {
      "foreground": "#7dc3e8"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6b8370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#f5e67a"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language",
        "punctuation.definition.block.js.jsx"
      ],
      "settings": {
        "foreground": "#d99ce8"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a8e6a3"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#cbd7d1"
      }
    },
    {
      "name": "JSX Spread Operator",
      "scope": [
        "keyword.operator.spread.js.jsx"
      ],
      "settings": {
        "foreground": "#f5c97a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX Template Expression",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.jsx",
        "punctuation.definition.template-expression.end.jsx",
        "punctuation.definition.template-expression.begin.js.jsx",
        "punctuation.definition.template-expression.end.js.jsx",
        "meta.template.expression",
        "meta.template.expression.js",
        "meta.template.expression.jsx",
        "meta.template.expression.js.jsx",
        "string.template",
        "string.template.js",
        "string.template.jsx",
        "string.template.js.jsx"
      ],
      "settings": {
        "foreground": "#a8e6a3"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7dd3c0"
      }
    },
    {
      "name": "Function Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#f5c97a"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#d9ebe0"
      }
    },
    {
      "name": "JSX Read/Write Variables",
      "scope": [
        "variable.other",
        "variable.other.readwrite",
        "variable.other.readwrite.js",
        "variable.other.readwrite.jsx",
        "variable.other.readwrite.js.jsx",
        "meta.embedded.line.js.jsx variable.other",
        "meta.template.expression.js.jsx variable.other",
        "string.template.js.jsx variable.other",
        "meta.block.js.jsx variable.other",
        "meta.function.js.jsx variable.other",
        "source.js.jsx variable.other"
      ],
      "settings": {
        "foreground": "#f5c97a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#7dc3e8"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#a8e6a3"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#7dc3e8"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator",
        "support.type.decorator"
      ],
      "settings": {
        "foreground": "#7dd3c0"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#a8e6a3"
      }
    },
    {
      "name": "Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#f5c97a"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#7dc3e8"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold",
        "markup.strong"
      ],
      "settings": {
        "foreground": "#f5e67a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic",
        "markup.emphasis"
      ],
      "settings": {
        "foreground": "#d99ce8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#7dd3c0"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.namespace",
        "support.other.namespace"
      ],
      "settings": {
        "foreground": "#7dc3e8"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "meta.brace.round.js.jsx"
      ],
      "settings": {
        "foreground": "#a8e6a3"
      }
    }
    ,
    {
      "name": "Punctuation / Brackets",
      "scope": [
        "punctuation.bracket",
        "punctuation.definition.brace",
        "punctuation.section.block",
        "meta.brace",
        "meta.brace.round",
        "meta.brace.square"
      ],
      "settings": {
        "foreground": "#8fc3a5"
      }
    }
  ]
}
